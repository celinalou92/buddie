{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 5
    }
  },

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
        "source": "/",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, s-maxage=1, stale-while-revalidate=59"
          }
        ]
    }
  ]
}
